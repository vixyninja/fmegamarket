{
  "name": "fmegamarket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "test": "jest",
    "format": "prettier --write .",
    "dev:setup": "ENV=development ./run.sh",
    "prod:setup": "ENV=production ./run.sh",
    "android:setup-dev": "yarn dev:setup && ENVFILE=configs/dev/.env.development",
    "android:setup-prod": "yarn prod:setup && ENVFILE=configs/prod/.env.production",
    "dev:debug": "yarn android:setup-dev react-native run-android --mode=debug",
    "prod:debug": "yarn android:setup-prod react-native run-android --mode=debug",
    "dev:release": "yarn android:setup-dev react-native run-android --mode=release",
    "prod:release": "yarn android:setup-prod react-native run-android --mode=release",
    "dev:apk": "yarn dev:setup && cd android && ENVFILE=configs/dev/.env.development ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:apk": "yarn prod:setup && cd android && ENVFILE=configs/prod/.env.development ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:aab": "yarn prod:setup && cd android && ENVFILE=configs/prod/.env.development ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "sign:dev": "cd android && ENVFILE=configs/dev/.env.development ./gradlew signingReport && cd ..",
    "sign:prod": "cd android && ENVFILE=configs/prod/.env.production ./gradlew signingReport && cd ..",
    "clean": "cd android && ENVFILE=configs/dev/.env.development ./gradlew clean && cd .."
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.0",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/devtools": "^6.0.20",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "async-mutex": "^0.4.0",
    "builder-pattern": "^2.2.0",
    "i18next": "^23.7.6",
    "lottie-react-native": "^6.4.1",
    "react": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.72.6",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-logs": "^5.0.1",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-native-vector-icons": "^10.0.2",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-redux": "^7.1.30",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.12",
    "@types/redux-persist": "^4.3.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.212.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "redux-logger": "^3.0.6",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
